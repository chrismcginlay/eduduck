    <script src="{% static 'common.js' %}"></script>
    <script src="{% static 'search.js' %}"></script>
{% block extra_script %}{% endblock extra_script %}

    <title>{% block title %}EduDuck{% endblock title %}</title>
</head>

<body>

        <header id="branding" title="Home">
          	{% if COMPLETION_RATIO > 50 %}
          		<a href='https://docs.google.com/forms/d/1VrUfHZ3oVFByCeZf19CNl_OY0po9gTuMlKIT20rKY48/viewform'>
                   <img id="survey" width='195' height='250' 
                        title='Complete our Survey!' alt='Complete our Survey!'
                        src="{% static 'images/SurveyIcon_195.png' %}">
                </a>
            {% endif %}

            {% if user.is_authenticated %}
                <p id="timezone">{{ TIME_ZONE }}</p>
            {% endif %}
            <div id="search">
            {% block search %}
                <form method="get" action="/search/">
                  <input type="search" name="q" id="id_q" value="Search with Elasticsearch" />
                  <input value="search" type="submit" id="search"/>
                </form>
            {% endblock search %}
            </div>
        </header>


        <div id="content">
            <h2>
            {% block contentheading %}
            A Page With No Title. Override Me
            {% endblock contentheading %}
            </h2>

            {% block content %}{% endblock content %}
        </div>



